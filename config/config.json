{
  "shader-constants": {
    "MAX_LIGHTS": 8
  },
  "defaults": {
    "defaultWindowWidth": 1920,
    "defaultWindowHeight": 1080,
    "gridScale": 2.0
  },
  "loggers": {
    "stdInfo": {
      "hasTimeStamp": true,
      "colour": "INFO_COLOUR",
      "type": "INFO"
    },
    "stdWarn": {
      "hasTimeStamp": true,
      "colour": "WARNING_COLOUR",
      "type": "WARNING"
    },
    "stdError": {
      "hasTimeStamp": true,
      "colour": "ERROR_COLOUR",
      "type": "ERROR"
    }
  }
}